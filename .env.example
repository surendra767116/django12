# Google OAuth Configuration
# Get these from https://console.cloud.google.com/
GOOGLE_OAUTH_CLIENT_ID=your-google-client-id-here.apps.googleusercontent.com
GOOGLE_OAUTH_CLIENT_SECRET=your-google-client-secret-here

# Admin Users (comma-separated email addresses)
# Users with these emails will have admin access
ADMIN_EMAILS=admin@example.com,another-admin@example.com

# Django Secret Key (change this in production)
SECRET_KEY=django-insecure-change-this-in-production

# Debug Mode (set to False in production)
DEBUG=True
